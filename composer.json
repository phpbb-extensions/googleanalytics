{
	"name": "phpbb/googleanalytics",
	"type": "phpbb-extension",
	"description": "An extension which adds Google Analytics to your phpBB forum",
	"homepage": "https://phpbb.com",
	"version": "1.0.0-dev",
	"keywords": ["phpbb", "extension", "google", "analytics"],
	"license": "GPL-2.0",
	"authors": [
